{
  "name": "conceptsandtraining/lib-plugin-security",
  "version": "v0.0.2",
  "type": "library",
  "description": "Library with security options for ilias plugin konfiguration",
  "license": [
      "GPL-3.0-or-later"
  ],
  "authors": [
    {
      "name": "Stefan Hecken",
      "email": "stefan.hecken@concepts-and-training.de"
    }
  ],
  "autoload" : {
    "psr-4" : {
      "CaT\\Security\\" : ["./src", "./tests"]
    }
  },
  "config" : {
    "autoloader-suffix" : "Security"
  },
  "require-dev" : {
    "phpunit/phpunit" : "^5.6",
    "sensiolabs/security-checker": "^5.0",
    "roave/security-advisories": "dev-master"
  }
}
